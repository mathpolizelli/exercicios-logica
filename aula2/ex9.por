programa {
  funcao inicio() {
    real vpm, l1, l2, l3

    escreva("Digite o valor do lado do prisma: ")
    leia(l1)
    escreva("Digite o valor do outro lado do prisma: ")
    leia(l2)
    escreva("Digite o valor do outro lado do prisma: ")
    leia(l3)

    vpm = l1 * l2 * l3
    escreva("O volume do prisma e igual a ", vpm)
  }
}
